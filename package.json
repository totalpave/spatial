{
  "private": true,
  "version": "1.0.0",
  "description": "a node.js library for performing geospatial operations with geojson",
  "scripts": {
    "clean": "find -type f -name 'yarn.lock' -delete && lerna clean --yes && yarn cache clean",
    "prelint": "tsc",
    "lint": "eslint packages/turf-*/index.js",
    "postlint": "documentation lint packages/turf-*/index.js",
    "pretest": "lerna bootstrap && npm run lint",
    "test": "lerna run test",
    "docs": "node ./scripts/generate-readmes",
    "5to6": "jscodeshift -t node_modules/5to6-codemod/transforms/cjs.js packages/turf-*/bench.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/turfjs/turf.git"
  },
  "keywords": [
    "gis",
    "geo",
    "geojs",
    "geospatial",
    "geography",
    "geometry",
    "map",
    "contour",
    "centroid",
    "tin",
    "extent",
    "geojson",
    "grid",
    "polygon",
    "line",
    "point",
    "area",
    "analysis",
    "statistics",
    "stats",
    "midpoint",
    "plane",
    "quantile",
    "jenks",
    "sample"
  ],
  "author": "morganherlocker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Turfjs/turf/issues"
  },
  "devDependencies": {
    "5to6-codemod": "*",
    "@mapbox/geojsonhint": "*",
    "@types/geojson": "*",
    "@types/node": "*",
    "@types/tape": "*",
    "babel-plugin-external-helpers": "*",
    "babel-preset-env": "*",
    "benchmark": "*",
    "boolean-jsts": "*",
    "boolean-shapely": "*",
    "camelcase": "*",
    "chroma-js": "*",
    "chromatism": "*",
    "concaveman": "*",
    "d3-queue": "*",
    "decamelize": "*",
    "documentation": "*",
    "eslint": "*",
    "eslint-config-mourner": "*",
    "esm": "^3.2.25",
    "fs-extra": "*",
    "geojson-fixtures": "*",
    "glob": "*",
    "jscodeshift": "*",
    "lerna": "^3",
    "load-json-file": "*",
    "matrix-to-grid": "*",
    "meow": "*",
    "mkdirp": "*",
    "progress": "*",
    "proj4": "*",
    "rollup": "*",
    "rollup-plugin-buble": "*",
    "rollup-plugin-commonjs": "*",
    "rollup-plugin-node-resolve": "*",
    "rollup-plugin-terser": "^4.0.4",
    "tape": "*",
    "typescript": "*",
    "write-json-file": "*",
    "yamljs": "*"
  },
  "esm": {
    "cjs": true
  },
  "name": "turf"
}
