{
  "name": "spatial",
  "private": true,
  "version": "1.0.0",
  "description": "a node.js library for performing geospatial operations with geojson",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "find -type f -name 'package-lock.lock' -delete && lerna clean --yes",
    "prelint": "tsc",
    "lint": "eslint packages/*/index.js",
    "pretest": "lerna bootstrap && npm run lint",
    "test": "lerna run test",
    "docs": "node ./scripts/generate-readmes"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/totalpave/spatial.git"
  },
  "keywords": [
    "gis",
    "geo",
    "geojs",
    "geospatial",
    "geography",
    "geometry",
    "map",
    "contour",
    "centroid",
    "tin",
    "extent",
    "geojson",
    "grid",
    "polygon",
    "line",
    "point",
    "area",
    "analysis",
    "statistics",
    "stats",
    "midpoint",
    "plane",
    "quantile",
    "jenks",
    "sample"
  ],
  "author": "norman.breau@totalpave.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/totalpave/spatial/issues"
  },
  "devDependencies": {
    "@mapbox/geojsonhint": "3.0.0",
    "@totalpave/eslint-plugin": "4.0.0",
    "@types/geojson": "7946.0.7",
    "@types/node": "12.19.6",
    "@types/tape": "4.2.33",
    "@typescript-eslint/eslint-plugin": "4.8.2",
    "@typescript-eslint/parser": "4.8.2",
    "benchmark": "2.1.4",
    "boolean-jsts": "0.0.1",
    "boolean-shapely": "0.1.2",
    "camelcase": "5.3.1",
    "chroma-js": "^2.1.0",
    "chromatism": "3.0.0",
    "concaveman": "^1.2.0",
    "d3-queue": "3.0.7",
    "decamelize": "3.2.0",
    "documentation": "11.0.1",
    "eslint": "7.14.0",
    "fs-extra": "^9.0.1",
    "geojson-fixtures": "1.0.0",
    "glob": "^7.1.6",
    "lerna": "^3.22.1",
    "load-json-file": "^6.2.0",
    "matrix-to-grid": "4.0.0",
    "meow": "5.0.0",
    "progress": "2.0.3",
    "proj4": "^2.6.2",
    "tape": "4.11.0",
    "typedoc": "0.19.2",
    "typescript": "4.1.2",
    "write-json-file": "^4.3.0",
    "yamljs": "0.3.0"
  }
}
