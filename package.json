    "progress": "2.0.3",
    "proj4": "^2.7.2",
    "rollup": "^2.44.0",
    "rollup-plugin-buble": "0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "tape": "^5.2.2",
    "typescript": "^4.2.3",
    "write-json-file": "^4.3.0",
