{
  "private": true,
  "version": "1.0.0",
  "description": "a node.js library for performing geospatial operations with geojson",
  "scripts": {
    "clean": "find -type f -name 'package-lock.lock' -delete && lerna clean --yes",
    "prelint": "tsc",
    "lint": "eslint packages/turf-*/index.js",
    "postlint": "documentation lint packages/turf-*/index.js",
    "pretest": "lerna bootstrap && npm run lint",
    "test": "lerna run test",
    "docs": "node ./scripts/generate-readmes",
    "5to6": "jscodeshift -t node_modules/5to6-codemod/transforms/cjs.js packages/turf-*/bench.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/turfjs/turf.git"
  },
  "keywords": [
    "gis",
    "geo",
    "geojs",
    "geospatial",
    "geography",
    "geometry",
    "map",
    "contour",
    "centroid",
    "tin",
    "extent",
    "geojson",
    "grid",
    "polygon",
    "line",
    "point",
    "area",
    "analysis",
    "statistics",
    "stats",
    "midpoint",
    "plane",
    "quantile",
    "jenks",
    "sample"
  ],
  "author": "morganherlocker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Turfjs/turf/issues"
  },
  "devDependencies": {
    "5to6-codemod": "1.8.0",
    "@mapbox/geojsonhint": "3.0.0",
    "@types/geojson": "7946.0.7",
    "@types/node": "^12.12.62",
    "@types/tape": "4.2.33",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-env": "1.7.0",
    "benchmark": "2.1.4",
    "boolean-jsts": "0.0.1",
    "boolean-shapely": "0.1.2",
    "camelcase": "5.3.1",
    "chroma-js": "^2.1.0",
    "chromatism": "3.0.0",
    "concaveman": "^1.2.0",
    "d3-queue": "3.0.7",
    "decamelize": "3.2.0",
    "documentation": "11.0.1",
    "eslint": "^7.9.0",
    "eslint-config-mourner": "3.0.0",
    "esm": "^3.2.25",
    "fs-extra": "8.1.0",
    "geojson-fixtures": "1.0.0",
    "glob": "^7.1.6",
    "jscodeshift": "0.6.4",
    "lerna": "^3.22.1",
    "load-json-file": "6.1.0",
    "matrix-to-grid": "4.0.0",
    "meow": "5.0.0",
    "mkdirp": "0.5.1",
    "progress": "2.0.3",
    "proj4": "2.5.0",
    "rollup": "1.16.4",
    "rollup-plugin-buble": "0.19.8",
    "rollup-plugin-commonjs": "10.0.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "^4.0.4",
    "tape": "4.11.0",
    "typescript": "3.6.2",
    "write-json-file": "^4.3.0",
    "yamljs": "0.3.0"
  },
  "esm": {
    "cjs": true
  },
  "name": "turf"
}
