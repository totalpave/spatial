{
  "private": true,
  "version": "1.0.0",
  "description": "a node.js library for performing geospatial operations with geojson",
  "scripts": {
    "clean": "find -type f -name 'package-lock.lock' -delete && lerna clean --yes",
    "prelint": "tsc",
    "lint": "eslint packages/turf-*/index.js",
    "postlint": "documentation lint packages/turf-*/index.js",
    "pretest": "lerna bootstrap && npm run lint",
    "test": "lerna run test",
    "docs": "node ./scripts/generate-readmes",
    "5to6": "jscodeshift -t node_modules/5to6-codemod/transforms/cjs.js packages/turf-*/bench.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/totalpave/spatial.git"
  },
  "keywords": [
    "gis",
    "geo",
    "geojs",
    "geospatial",
    "geography",
    "geometry",
    "map",
    "contour",
    "centroid",
    "tin",
    "extent",
    "geojson",
    "grid",
    "polygon",
    "line",
    "point",
    "area",
    "analysis",
    "statistics",
    "stats",
    "midpoint",
    "plane",
    "quantile",
    "jenks",
    "sample"
  ],
  "author": "morganherlocker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/totalpave/spatial/issues"
  },
  "devDependencies": {
    "5to6-codemod": "1.8.0",
    "@babel/plugin-transform-object-assign": "^7.12.13",
    "@babel/plugin-transform-template-literals": "^7.13.0",
    "@babel/preset-env": "^7.13.12",
    "@mapbox/geojsonhint": "3.0.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@types/geojson": "7946.0.7",
    "@types/node": "^14.14.37",
    "@types/tape": "^4.13.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-env": "1.7.0",
    "benchmark": "2.1.4",
    "boolean-jsts": "0.0.1",
    "boolean-shapely": "0.1.2",
    "camelcase": "^6.2.0",
    "chroma-js": "^2.1.1",
    "chromatism": "3.0.0",
    "concaveman": "^1.2.0",
    "d3-queue": "3.0.7",
    "decamelize": "^5.0.0",
    "documentation": "^13.2.0",
    "eslint": "^7.23.0",
    "eslint-config-mourner": "3.0.0",
    "esm": "^3.2.25",
    "fs-extra": "^9.1.0",
